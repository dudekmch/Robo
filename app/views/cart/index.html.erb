 <table class="table table-striped">
        <thead>
            <tr>
                <th><%= sort_link(@search, :id, "Numer zamówienia") %></th>
                <th><%= sort_link(@search, :created_at, "Data złozenia zamówienia") %></th>
            </tr>
        </thead>
        <tbody>
            <% @orders.each do |order| %>
                <tr>
                    <td><%= order.id %></td>
                    <td><%= order.created_at %></td>
                </tr>
            <% end %>
        </tbody>
    </table>